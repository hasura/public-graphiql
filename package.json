{
  "name": "@hasura/public-graphiql",
  "version": "0.1.3",
  "files": [
    "dist",
    "README.md"
  ],
  "private": false,
  "main": "dist/HasuraGraphiQL.js",
  "module": "dist/HasuraGraphiQL.js",
  "dependencies": {
    "@graphiql/toolkit": "^0.4.0",
    "graphiql": "^1.4.6",
    "graphiql-code-exporter": "^3.0.3",
    "graphiql-explorer": "^0.6.3",
    "graphql-language-service-utils": "^2.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hasura/public-graphiql"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --extensions '.ts,.tsx'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@cypress/react": "^5.10.3",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "cypress": "^8.7.0",
    "graphql": "^15.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "peerDependencies": {
    "graphql": "15.x",
    "react": "16.x",
    "react-dom": "16.x"
  }
